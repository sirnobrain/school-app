<%- include('templates/header.ejs') %>

<div>
	<table>
		<thead>
			<tr>
				<th>Id</th>
				<th>First Name</th>
				<th>Last Name</th>
				<th>Email</th>
			</tr>
		</thead>
		<tbody>
			<%teachers.forEach(teacher => {%>
			<tr>
				<td><%= teacher.id %></td>
				<td><%= teacher.first_name %></td>
				<td><%= teacher.last_name %></td>
				<td><%= teacher.email %></td>
			</tr>
			<%})%>
		</tbody>
	</table>
</div>

<%- include('templates/footer.ejs') %>